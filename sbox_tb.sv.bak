`timescale 1ns / 1ps

module sbox_tb;

  // Dùng logic thay cho reg và wire
  logic [7:0] data;
  logic [7:0] sbox_out;

  // Gọi module AES1 (DUT - Device Under Test)
  sbox dut (
    .a(data),
    .c(sbox_out),
  );

  // Sinh tín hiệu kiểm tra
  initial begin
    // Test case 1
    plaintext = 7'h0e;
    #20; // chờ mô phỏng 20ns
  end


endmodule